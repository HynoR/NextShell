{
  "name": "nextshell",
  "version": "0.1.6",
  "private": true,
  "description": "NextShell desktop prototype workspace",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "setup": "bun install && bun run --cwd apps/desktop rebuild:native",
    "rebuild:native": "bun run --cwd apps/desktop rebuild:native",
    "dev": "bun run --cwd apps/desktop dev",
    "build": "bun run --cwd apps/desktop build",
    "typecheck": "bun run --cwd apps/desktop typecheck"
  },
  "engines": {
    "bun": ">=1.1.0"
  },
  "packageManager": "bun@1.3.4",
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/utilities": "^3.2.2"
  }
}
