{
  "name": "@nextshell/desktop",
  "version": "0.1.0",
  "private": true,
  "description": "NextShell desktop application",
  "author": "NextShell Team",
  "repository": {
    "type": "git",
    "url": "https://github.com/HynoR/NextShell.git"
  },
  "type": "module",
  "main": "dist-electron/main/index.js",
  "scripts": {
    "dev": "vite",
    "build": "tsc --noEmit -p tsconfig.node.json && tsc --noEmit -p tsconfig.web.json && vite build",
    "dist": "bun run build && electron-builder --config build/electron-builder.yml",
    "dist:dir": "bun run build && electron-builder --dir --config build/electron-builder.yml",
    "typecheck": "tsc --noEmit -p tsconfig.node.json && tsc --noEmit -p tsconfig.web.json",
    "preview": "vite preview"
  },
  "dependencies": {
    "@codemirror/lang-css": "^6.3.1",
    "@codemirror/lang-html": "^6.4.9",
    "@codemirror/lang-javascript": "^6.2.2",
    "@codemirror/lang-json": "^6.0.1",
    "@codemirror/lang-markdown": "^6.3.1",
    "@codemirror/lang-php": "^6.0.1",
    "@codemirror/lang-python": "^6.1.6",
    "@codemirror/lang-yaml": "^6.1.2",
    "@codemirror/language": "^6.10.8",
    "@codemirror/legacy-modes": "^6.4.2",
    "@codemirror/state": "^6.5.4",
    "@codemirror/theme-one-dark": "^6.1.2",
    "@codemirror/view": "^6.39.15",
    "@tailwindcss/vite": "^4.2.0",
    "@xterm/addon-clipboard": "^0.2.0",
    "@xterm/addon-fit": "^0.11.0",
    "@xterm/addon-search": "^0.16.0",
    "@xterm/addon-webgl": "^0.19.0",
    "@xterm/xterm": "^6.0.0",
    "antd": "^6.3.0",
    "better-sqlite3": "^12.6.2",
    "chokidar": "^5.0.0",
    "codemirror": "^6.0.1",
    "crypto-js": "^4.2.0",
    "electron-log": "^5.4.3",
    "font-logos": "^1.3.0",
    "iconv-lite": "^0.7.0",
    "keytar": "^7.9.0",
    "ping": "^1.0.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-resizable-panels": "^4.6.4",
    "remixicon": "^4.9.1",
    "socks": "^2.8.7",
    "ssh2": "^1.17.0",
    "zod": "^4.3.6",
    "zustand": "^5.0.11"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.2.2",
